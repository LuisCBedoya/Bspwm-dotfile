{
    "workbench.startupEditor": "none",
    "editor.fontFamily": "'Victor Mono'",
    "editor.fontLigatures": "'ss01'",
    "editor.tabSize": 4,
    "editor.fontWeight": "bold",
    "editor.fontSize": 16,
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.fontFamily": "Hack Nerd Font",
    "workbench.sideBar.location": "right",
    "editor.cursorBlinking": "expand",
    "workbench.colorTheme": "Tokyo Night",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.productIconTheme": "productIconThemes",
     // map key vim extension
    // *************************************************
    "vim.easymotion": true,
    "vim.sneak": true,
    "vim.cursorStylePerMode.normal": "block-outline",
    "vim.incsearch": true,
    "vim.smartRelativeLine": true,
    "vim.useSystemClipboard": true,
    "vim.easymotionMarkerBackgroundColor": "#020202",
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    //MODO INSERT
    // *************************************************
    "vim.insertModeKeyBindings": [
        //disable Modo Insert 
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    //MODO NORMAL
    // *************************************************
    "vim.normalModeKeyBindingsNonRecursive": [
        //Activar modo insert
        {
            "before": [
                "j",
                "k"
            ],
            "after": [
                "i"
            ]
        },
        //Mover hacia arriba
        {
            "before": [
                "w"
            ],
            "after": [
                "k"
            ]
        }, //Mover hacia Abajo
        {
            "before": [
                "s"
            ],
            "after": [
                "j"
            ]
        },
        //Mover Hacia Izqierda
        {
            "before": [
                "a"
            ],
            "after": [
                "h"
            ]
        },
        //Mover hacia Derecha
        {
            "before": [
                "d"
            ],
            "after": [
                "l"
            ]
        }, 
        //Dejamos por defecto e de vim
        //Salto hacia el atras de la palabra 
        {
            "before": [
                "q"
            ],
            "after": [
                "b"
            ]
        },
        //recorrer entre palabras 
        {
            "before": [
                "x"
            ],
            "after": [
                "w"
            ]
        },
        //Añadir hacia el pricipio de la linea <- despierta el modo insert  
        {
            "before": [
                "leader",
                "leader",
                "e"
            ],
            "after": [
                "A"
            ]
        },
        //Añadir al final de la linea <- despierta modo insert 
        {
            "before": [
                "leader",
                "leader",
                "q"
            ],
            "after": [
                "I"
            ]
        },
        //Añadir linea hacia abajo se deja por defecto que es la o (Minuscula)
        //Añadir linea hacia Arriba
        {
            "before": [
                "leader",
                "o"
            ],
            "after": [
                "O"
            ]
        },
        //Eliminar Linea 
        {
            "before": [
                "z",
                "z"
            ],
            "after": [
                "d",
                "d"
            ]
        },
        //Dejamos la r (minuscula) por defecto vim para remplazar una letra
        //Remplazar Linea
        {
            "before": [
                "leader",
                "r"
            ],
            "after": [
                "R"
            ]
        },
        //Copiar linea
        {
            "before": [
                "c"
            ],
            "after": [
                "y",
                "y"
            ]
        },
        //pegar hacia abajo
        {
            "before": [
                "ñ"
            ],
            "after": [
                "p"
            ]
        },
        //pegar hacia arriba
        {
            "before": [
                "p"
            ],
            "after": [
                "P"
            ]
        },
        //Ir al inicio del archivo
        {
            "before": [
                "n"
            ],
            "after": [
                "g",
                "g"
            ]
        },
        //Ir al final del archivo
        {
            "before": [
                "m"
            ],
            "after": [
                "G"
            ]
        },
        //salto de linea al inicio 
        {
            "before": [
                "leader",
                "e"
            ],
            "after": [
                "$"
            ]
        },
        //salto de linea hacia el final de la linea 
        {
            "before": [
                "leader",
                "q"
            ],
            "after": [
                "0"
            ]
        },
        //borrar toda la linea hacia atras 
        {
            "before": [
                "z",
                "q"
            ],
            "after": [
                "d",
                "0"
            ]
        },
        //borrar linea hacia adelante  
        {
            "before": [
                "z",
                "e"
            ],
            "after": [
                "d",
                "$"
            ]
        },
        //borrar palabra de adelante del cursos  
        {
            "before": [
                "z",
                "d"
            ],
            "after": [
                "d",
                "w"
            ]
        },
        //copiar palabra delante del cursor  
        {
            "before": [
                "c",
                "d"
            ],
            "after": [
                "y",
                "w"
            ]
        },
        //copiar una letra delante del cursos  
        {
            "before": [
                "c",
                "f"
            ],
            "after": [
                "y",
                "l"
            ]
        },
        //copiar la linea hacia adelante desde donde este el cursor  
        {
            "before": [
                "c",
                "e"
            ],
            "after": [
                "y",
                "$"
            ]
        },
        //copiar la linea hacia atras desde donde este el cursor  
        {
            "before": [
                "c",
                "q"
            ],
            "after": [
                "y",
                "0"
            ]
        },
        //borrar hacia arriba
        {
            "before": [
                "z",
                "w"
            ],
            "after": [
                "d",
                "G"
            ]
        },
        //Mostrar llave que cierra o llave que abre 
        {
            "before": [
                "t"
            ],
            "after": [
                "%"
            ]
        },
        //tabular hacia adelante
        {
            "before": [
                "l"
            ],
            "after": [
                ">"
            ]
        },
        //tabular hacia atras
        {
            "before": [
                "k"
            ],
            "after": [
                "<"
            ]
        },
        //Entrar en modo visualline
        {
            "before": [
                "leader",
                "v"
            ],
            "after": [
                "V"
            ]
        },
        {
            "before": [
                "<C-n>"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                "leader",
                "k"
            ],
            "commands": [
                "lineBreakInsert"
            ],
            "silent": true
        }
    ],
